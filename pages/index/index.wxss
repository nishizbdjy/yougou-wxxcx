/**index.wxss**/

.search {
  padding: 20rpx;
  background: #ff2d4a;
}

.input {
  background: #fff;
  width: 100%;
  height: 60rpx;
  border-radius: 4px;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 12px;
  color: #c7c7c7;
}

/*/////轮播图*/

.swiper {
  width: 100%;
  height: 340rpx;
}

.swiper image {
  width: 100%;
  height: 340rpx;
}

/* 导航栏 */

.navList {
  width: 100%;
  height: 180rpx;
  padding: 20rpx 0;
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.navList-fils {
  width: 150rpx;
  height: 180rpx;
}

.navList-fils navigator{
  width: 100%;
  height: 100%;
}
.navList-fils image {
  width: 100%;
  height: 100%;
}
